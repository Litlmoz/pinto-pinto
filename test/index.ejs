<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script>
      const ROOM_ID = '<%= roomId %>';
      const STUN_URL = '<%= stunUrl %>';
      const KEY = '<%= key %>';
      const HOST = '<%= host %>';
      const ENVIROMENT = '<%= env %>';
    </script>
    <script
      type="application/json"
      src="https://cdnjs.cloudflare.com/ajax/libs/peerjs/1.3.2/peerjs.min.js.map"
    ></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/peerjs/1.3.2/peerjs.min.js"
    integrity="sha512-4wTQ8feow93K3qVGVXUGLULDB9eAULiG+xdbaQH8tYZlXxYv9ij+evblXD0EOqmGWT8NBTd1vQGsURvrQzmKeg=="
    crossorigin="anonymous"
    ></script>
    <script
      type="application/json"
      src="https://cdn.socket.io/4.1.2/socket.io.min.js.map"
    ></script>
    <script
      defer
      type="text/javascript"
      src="https://cdn.socket.io/4.1.2/socket.io.min.js"
      crossorigin="anonymous"
    ></script>
    <script defer type="text/javascript" src="/script.js"></script>
    <title>WebRTC Test</title>
    <style>
      #video-grid {
        display: grid;
        grid-template-columns: repeat(auto-fill, 300px);
        grid-auto-rows: 300px;
      }
      video {
        width: 100%;
        height: 100%;
        object-fit: cover;
      }
    </style>
  </head>
  <body>
    <div id="video-grid"></div>
    <ul id="connected-users"></ul>
    <div id="call-controls"></div>
  </body>
</html>
